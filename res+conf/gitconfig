[color]
    ui = true
[user]
    #name = NAME
    #email = EMAIL
[core]
    excludesfile = ~/.gitignore
    whitespace = trailing-space
[alias]
    alias = !git config --get-regexp '^alias\\.' |sed -e 's:^alias\\.::' -e '/^alias/d' -e 's:\\ :\\t:' |column -ts '\t'
    s = status --short
    brieflog = !GIT_PAGER=cat git log --oneline -10
    graph = log --all --oneline --decorate --graph
    reset-unmerged = !git reset -- $(git ls-files --unmerged |cut -f2 |uniq)
    fixup = !GIT_EDITOR=true git rebase --autosquash -i
    l = !git brieflog
    d = diff --color-words
    ds = !git d --staged
    co = checkout
    br = branch
[push]
    default = simple
[rebase]
    autosquash = true
[grep]
    patternType = perl
    lineNumber = true
